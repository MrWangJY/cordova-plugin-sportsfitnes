<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-sportsfitnes" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>SPortsFitness</name>
    <js-module name="SPortsFitness" src="www/SPortsFitness.js">
        <clobbers target="cordova.plugins.SPortsFitness" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="SPortsFitness">
                <param name="ios-package" value="SPortsFitness" />
            </feature>
        </config-file>
        <header-file src="src/ios/SPortsFitness.h" />
        <source-file src="src/ios/SPortsFitness.m" />
    </platform>
</plugin>
